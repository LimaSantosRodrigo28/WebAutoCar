@model IEnumerable<WebAutoCar.Models.Acessorios>

<p>
    @Html.ActionLink("Adicionar Novo Acessorio", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdAcessorios)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.IdAcessorios)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Nome)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.IdAcessorios }) |
        @Html.ActionLink("Deletar", "Delete", new { id = item.IdAcessorios })
    </td>
</tr>
}

</table>
